pipeline {
    agent any

    stages {
        
        stage('code pull from main branch') {
            steps {
                git branch: 'main', url: 'https://github.com/prem-devopser/branch-demo/'
            }
        }
        
        
        stage('cat file 1') {
            steps {
                sh 'cat file1.txt'
            }
        }
        
        stage('cat file 2') {
            steps {
                sh 'cat file2.txt'
            }
        }
    }
}
